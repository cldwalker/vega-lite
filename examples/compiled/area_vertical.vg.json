{ data: 'main', field: 'year_Year' }
{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "description": "Area chart showing weight of cars over time (vertical).",
  "background": "white",
  "padding": 5,
  "width": 200,
  "height": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/cars.json",
      "format": {"type": "json", "parse": {"Year": "date"}},
      "transform": [
        {
          "field": "Year",
          "type": "timeunit",
          "units": ["year"],
          "as": ["year_Year", "year_Year_end"]
        },
        {
          "type": "aggregate",
          "groupby": ["year_Year"],
          "ops": ["sum"],
          "fields": ["Weight_in_lbs"],
          "as": ["sum_Weight_in_lbs"]
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "marks",
      "type": "area",
      "style": ["area"],
      "sort": {"field": "datum[\"year_Year\"]"},
      "from": {"data": "source_0"},
      "encode": {
        "update": {
          "interpolate": {"value": "monotone"},
          "orient": {"value": "horizontal"},
          "fill": {"value": "#4c78a8"},
          "x": {"scale": "x", "field": "sum_Weight_in_lbs"},
          "x2": {"scale": "x", "value": 0},
          "y": {"scale": "y", "field": "year_Year"},
          "defined": {
            "signal": "isValid(datum[\"sum_Weight_in_lbs\"]) && isFinite(+datum[\"sum_Weight_in_lbs\"]) && isValid(datum[\"year_Year\"]) && isFinite(+datum[\"year_Year\"])"
          }
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {"data": "source_0", "field": "sum_Weight_in_lbs"},
      "range": [0, {"signal": "width"}],
      "nice": true,
      "zero": true
    },
    {
      "name": "y",
      "type": "time",
      "domain": {"data": "source_0", "field": "year_Year"},
      "range": [{"signal": "height"}, 0]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "gridScale": "y",
      "grid": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Sum of Weight_in_lbs",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Year (year)",
      "format": {
        "signal": "timeUnitSpecifier([\"year\"], {\"year-month\":\"%b %Y \",\"year-month-date\":\"%b %d, %Y \"})"
      },
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 0
    }
  ]
}
