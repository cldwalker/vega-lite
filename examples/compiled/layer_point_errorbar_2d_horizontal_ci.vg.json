{ data: 'layer_1_main', field: 'mean_yield' }
{ data: 'layer_0_main', field: 'variety', sort: true }
{ data: 'layer_1_main', field: 'variety', sort: true }
{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "width": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/barley.json",
      "format": {"type": "json"},
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["variety"],
          "ops": ["mean", "mean", "ci0", "ci1"],
          "fields": ["yield", "yield", "yield", "yield"],
          "as": ["mean_yield", "center_yield", "lower_yield", "upper_yield"]
        }
      ]
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"mean_yield\"]) && isFinite(+datum[\"mean_yield\"])"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"lower_yield\"]) && isFinite(+datum[\"lower_yield\"])"
        }
      ]
    }
  ],
  "signals": [
    {"name": "y_step", "value": 20},
    {"name": "height", "update": "bandspace(domain('y').length, 0, 0) * y_step"}
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "rule",
      "style": ["rule", "errorbar-rule"],
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "stroke": {"value": "black"},
          "tooltip": {
            "signal": "{\"Ci1 of yield\": format(datum[\"upper_yield\"], \"\"), \"Ci0 of yield\": format(datum[\"lower_yield\"], \"\"), \"Mean of yield\": format(datum[\"center_yield\"], \"\"), \"variety\": isValid(datum[\"variety\"]) ? datum[\"variety\"] : \"\"+datum[\"variety\"]}"
          },
          "x": [
            {
              "test": "!isValid(datum[\"lower_yield\"]) || !isFinite(+datum[\"lower_yield\"])",
              "value": 0
            },
            {"scale": "x", "field": "lower_yield"}
          ],
          "x2": [
            {
              "test": "!isValid(datum[\"upper_yield\"]) || !isFinite(+datum[\"upper_yield\"])",
              "value": 0
            },
            {"scale": "x", "field": "upper_yield"}
          ],
          "y": {"scale": "y", "field": "variety", "band": 0.5}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "symbol",
      "style": ["point"],
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "fill": {"value": "black"},
          "x": [
            {
              "test": "!isValid(datum[\"mean_yield\"]) || !isFinite(+datum[\"mean_yield\"])",
              "value": 0
            },
            {"scale": "x", "field": "mean_yield"}
          ],
          "y": {"scale": "y", "field": "variety", "band": 0.5}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "data_1", "field": "lower_yield"},
          {"data": "data_1", "field": "upper_yield"},
          {"data": "data_0", "field": "mean_yield"}
        ]
      },
      "range": [0, {"signal": "width"}],
      "zero": false,
      "nice": true
    },
    {
      "name": "y",
      "type": "band",
      "domain": {
        "fields": [
          {"data": "data_1", "field": "variety"},
          {"data": "data_0", "field": "variety"}
        ],
        "sort": true
      },
      "range": {"step": {"signal": "y_step"}},
      "paddingInner": 0,
      "paddingOuter": 0
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "gridScale": "y",
      "grid": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "yield",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "variety",
      "labelOverlap": true,
      "zindex": 0
    }
  ]
}
