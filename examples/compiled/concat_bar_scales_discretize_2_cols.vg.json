      "range": {"scheme": "magma"},
      "interpolate": "hcl"
    },
    {
      "name": "concat_1_size",
      "type": "quantile",
      "domain": {"data": "data_2", "field": "b"},
      "range": [80, 160, 240, 320, 400]
    },
    {
      "name": "concat_2_y",
