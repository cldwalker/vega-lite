{ data: 'main', field: 'mean_Horsepower' }
{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "description": "A scatterplot showing horsepower and miles per gallons for various cars.",
  "background": "white",
  "padding": 5,
  "width": 200,
  "height": 200,
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "data/cars.json",
      "format": {"type": "json", "parse": {"Year": "date"}},
      "transform": [
        {
          "type": "extent",
          "field": "Acceleration",
          "signal": "bin_maxbins_6_Acceleration_extent"
        },
        {
          "type": "bin",
          "field": "Acceleration",
          "as": [
            "bin_maxbins_6_Acceleration",
            "bin_maxbins_6_Acceleration_end"
          ],
          "signal": "bin_maxbins_6_Acceleration_bins",
          "extent": {"signal": "bin_maxbins_6_Acceleration_extent"},
          "maxbins": 6
        },
        {
          "type": "aggregate",
          "groupby": [
            "Year",
            "bin_maxbins_6_Acceleration",
            "bin_maxbins_6_Acceleration_end"
          ],
          "ops": ["mean"],
          "fields": ["Horsepower"],
          "as": ["mean_Horsepower"]
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "pathgroup",
      "type": "group",
      "from": {
        "facet": {
          "name": "faceted_path_main",
          "data": "source_0",
          "groupby": ["bin_maxbins_6_Acceleration"]
        }
      },
      "encode": {
        "update": {
          "width": {"field": {"group": "width"}},
          "height": {"field": {"group": "height"}}
        }
      },
      "marks": [
        {
          "name": "marks",
          "type": "line",
          "style": ["line"],
          "sort": {"field": "datum[\"Year\"]"},
          "from": {"data": "faceted_path_main"},
          "encode": {
            "update": {
              "stroke": {
                "signal": "scale(\"color\", 0.5 * datum[\"bin_maxbins_6_Acceleration\"] + 0.5 * datum[\"bin_maxbins_6_Acceleration_end\"])"
              },
              "x": {"scale": "x", "field": "Year"},
              "y": {"scale": "y", "field": "mean_Horsepower"},
              "defined": {
                "signal": "isValid(datum[\"Year\"]) && isFinite(+datum[\"Year\"]) && isValid(datum[\"mean_Horsepower\"]) && isFinite(+datum[\"mean_Horsepower\"])"
              }
            }
          }
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "time",
      "domain": {"data": "source_0", "field": "Year"},
      "range": [0, {"signal": "width"}]
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {"data": "source_0", "field": "mean_Horsepower"},
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "color",
      "type": "bin-ordinal",
      "range": "ramp",
      "bins": {"signal": "bin_maxbins_6_Acceleration_bins"},
      "interpolate": "hcl"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "gridScale": "y",
      "grid": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Year",
      "labelFlush": true,
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(width/40)"},
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Mean of Horsepower",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 0
    }
  ],
  "legends": [
    {
      "stroke": "color",
      "symbolType": "stroke",
      "title": "Acceleration (binned)"
    }
  ]
}
