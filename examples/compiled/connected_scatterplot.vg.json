    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"miles\"]) && isFinite(+datum[\"miles\"]) && isValid(datum[\"gas\"]) && isFinite(+datum[\"gas\"])"
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "line",
      "style": ["line"],
      "sort": {"field": ["datum[\"year\"]"], "order": ["ascending"]},
      "from": {"data": "source_0"},
      "encode": {
        "update": {
          "stroke": {"value": "#4c78a8"},
          "x": {"scale": "x", "field": "miles"},
          "y": {"scale": "y", "field": "gas"},
          "defined": {
            "signal": "isValid(datum[\"miles\"]) && isFinite(+datum[\"miles\"]) && isValid(datum[\"gas\"]) && isFinite(+datum[\"gas\"])"
          }
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "symbol",
      "style": ["point"],
      "sort": {"field": ["datum[\"year\"]"], "order": ["ascending"]},
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "opacity": {"value": 1},
          "fill": {"value": "#4c78a8"},
          "x": [
            {
              "test": "!isValid(datum[\"miles\"]) || !isFinite(+datum[\"miles\"])",
              "value": 0
            },
            {"scale": "x", "field": "miles"}
          ],
          "y": [
            {
              "test": "!isValid(datum[\"gas\"]) || !isFinite(+datum[\"gas\"])",
              "field": {"group": "height"}
            },
            {"scale": "y", "field": "gas"}
          ]
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "source_0", "field": "miles"},
          {"data": "data_0", "field": "miles"}
        ]
      },
      "range": [0, {"signal": "width"}],
      "zero": false,
      "nice": true
    },
    {
      "name": "y",
